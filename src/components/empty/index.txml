<view class="empty {{className}}">
  <image class="empty-img" src="/assets/images/empty.png" mode="widthFix" />
  <view class="empty-text">
    <text class="empty-text-title">{{title}}</text>
    <text>{{description}}</text>
    <view tiki:if="{{buttonText}}" class="empty-continue-btn" onTap="_onTapButton">{{buttonText}}</view>
  </view>
  <view tiki:if="{{showBottomDivider}}" class="empty-bottom-divider" />
</view>